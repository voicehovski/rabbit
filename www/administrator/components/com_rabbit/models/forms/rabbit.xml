<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>
		<field
				name="id"
				type="hidden"
				/>
		<!--
			enctype="multipart/form-data"
			method="post"
			See also https://docs.joomla.org/File_form_field_type
		-->
		<field
				name="import_file"
				type="file"
				label="COM_RABBIT_RABBIT_FILE_LABEL"
				description="COM_RABBIT_RABBIT_FILE_DESC"
